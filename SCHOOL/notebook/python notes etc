def scribeSay(message, vibe="default", tag="scribe"):
    emote = random.choice(SCRIBE_EMOTES.get(vibe, SCRIBE_EMOTES["default"]))
    timestamp = time.strftime("%H:%M:%S")
    print(f"{emote} [{tag.upper()}] {timestamp} â€” {message}")

